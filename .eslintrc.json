{
  "rules": {
    "arrow-body-style": "off",
    "import/extensions": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-restricted-globals": "off",
    "no-console": "off",
    "no-undef": "off"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb", "eslint:recommended", "prettier"]
}
